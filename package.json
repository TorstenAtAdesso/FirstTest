{
  "name": "firsttest-svelte5-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:scaffold": "node scripts/validate-scaffold.mjs",
    "test": "npm run test:unit && npm run test:component",
    "test:coverage": "node --test --experimental-test-coverage tests/diff.test.js",
    "test:unit": "node --test tests/diff.test.js",
    "test:component": "vitest run tests/App.test.js"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "svelte": "^5.0.0",
    "vite": "^5.4.0",
    "@testing-library/svelte": "^5.2.8",
    "jsdom": "^25.0.1",
    "vitest": "^2.1.8"
  },
  "dependencies": {
    "@humanspeak/svelte-diff-match-patch": "^0.0.3"
  }
}
